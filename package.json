{
  "name": "mantine-resource-timeline",
  "description": "A resource timeline component built with Mantine",
  "private": false,
  "version": "0.2.0",
  "type": "module",
  "repository": {
    "url": "git+https://github.com/jvllmr/mantine-resource-timeline.git",
    "type": "git"
  },
  "main": "./build/index.cjs",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "author": "Jan Vollmer <jan@vllmr.dev>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "prepublish": "tsup",
    "test:build": "publint --strict",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "build",
    "src"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./build/index.d.ts",
        "default": "./build/index.js"
      },
      "require": {
        "types": "./build/index.d.cts",
        "default": "./build/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "@mantine/core": "^7.10.2",
    "@mantine/hooks": "^7.10.2",
    "@tanstack/react-virtual": "^3.9.0",
    "@use-gesture/react": "^10.3.1",
    "dayjs": "^1.11.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/eslint-parser": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@mantine/core": "^7.13.2",
    "@mantine/dates": "^7.13.2",
    "@mantine/hooks": "^7.13.2",
    "@storybook/addon-docs": "^8.3.5",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-storysource": "^8.3.5",
    "@storybook/addon-themes": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/preview-api": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@storybook/react-vite": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.2",
    "dayjs": "^1.11.13",
    "eslint": "^8.57.1",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.47",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "publint": "^0.2.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.3.5",
    "storybook-addon-mantine": "^4.0.2",
    "storybook-dark-mode": "^4.0.2",
    "tsup": "8.2.4",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  },
  "keywords": [
    "mantine",
    "resource",
    "scheduler",
    "timeline",
    "calendar",
    "component",
    "react"
  ],
  "packageManager": "pnpm@9.10.0"
}
