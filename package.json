{
  "name": "mantine-resource-timeline",
  "description": "A resource timeline component built with Mantine",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "repository": {
    "url": "git+https://github.com/jvllmr/mantine-resource-scheduler.git",
    "type": "git"
  },
  "main": "./build/index.cjs",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "author": "Jan Vollmer <jan@vllmr.dev>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "prepublish": "tsup",
    "test:build": "publint --strict",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "build",
    "src"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./build/index.d.ts",
        "default": "./build/index.js"
      },
      "require": {
        "types": "./build/index.d.cts",
        "default": "./build/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "@mantine/core": "^7.10.2",
    "@mantine/hooks": "^7.10.2",
    "@use-gesture/react": "^10.3.1",
    "dayjs": "^1.11.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@chromatic-com/storybook": "^1.5.0",
    "@mantine/core": "^7.10.2",
    "@mantine/dates": "^7.10.2",
    "@mantine/hooks": "^7.10.2",
    "@storybook/addon-essentials": "^8.1.9",
    "@storybook/addon-interactions": "^8.1.9",
    "@storybook/addon-links": "^8.1.9",
    "@storybook/addon-onboarding": "^8.1.9",
    "@storybook/blocks": "^8.1.9",
    "@storybook/react": "^8.1.9",
    "@storybook/react-vite": "^8.1.9",
    "@storybook/test": "^8.1.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react": "^4.3.1",
    "dayjs": "^1.11.11",
    "esbuild-plugin-file-path-extensions": "^2.1.2",
    "eslint": "^8.57.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "publint": "^0.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.1.9",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.3.1"
  },
  "keywords": [
    "mantine",
    "resource",
    "scheduler",
    "timeline",
    "calendar",
    "component",
    "react"
  ]
}
